{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"excludePattern": "(build|config|node_modules|static)",
		"includePattern": "\\.(vue|js)$",
		"include": ["README.md"]
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/jsdoc-vue"
	],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}